{
  "name": "market-engine",
  "version": "0.10.0",
  "description": "event-driven market engine for economic research, simulations, and experiments",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf ./build ./doc ; mkdir ./build",
    "build": "node_modules/.bin/babel build src/index.js -o build/index.js",
    "doc": "node_modules/.bin/esdoc -c esdoc.json",
    "lint": "node_modules/.bin/eslint src/index.js",
    "lint-test": "node_modules/.bin/eslint test/index.js",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register --reporter spec --slow 50 --timeout 60000",
    "cover": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports --compilers js:babel-register --timeout 60000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drpaulbrewer/market-engine.git"
  },
  "keywords": [
    "exchange",
    "market",
    "market-simulation",
    "financial-simulation",
    "experimental-economics",
    "mircoeconomics",
    "market-microstructure",
    "market-design"
  ],
  "author": "drpaulbrewer@eaftc.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drpaulbrewer/market-engine/issues"
  },
  "homepage": "https://github.com/drpaulbrewer/market-engine#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "coveralls": "^2.11.9",
    "esdoc": "^0.4.7",
    "eslint": "^3.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "should": "^8.3.1"
  }
}
